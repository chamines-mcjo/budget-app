{
  "name": "budget-app",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "test:ci": "jest",
    "test:ci:coverage": "jest --coverage",
    "lint": "expo lint",
    "ts:check": "tsc -p . --noEmit",
    "storybook": "npm run storybook-generate && STORYBOOK_ENABLED='true' expo start",
    "storybook-generate": "sb-rn-get-stories"
  },
  "jest": {
    "preset": "jest-expo",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{ts,tsx,js,jsx}",
      "!**/coverage/**",
      "!**/stories/**",
      "!**/node_modules/**",
      "!**/babel.config.js",
      "!**/expo-env.d.ts",
      "!**/.expo/**"
    ]
  },
  "dependencies": {
    "@expo-google-fonts/inter": "^0.2.3",
    "@expo/vector-icons": "^15.0.3",
    "@gorhom/bottom-sheet": "^5.2.6",
    "@react-navigation/bottom-tabs": "^7.0.0",
    "@react-navigation/native": "^7.0.0",
    "expo": "^54.0.0",
    "expo-blur": "~15.0.7",
    "expo-constants": "~18.0.10",
    "expo-font": "~14.0.9",
    "expo-haptics": "~15.0.7",
    "expo-linear-gradient": "~15.0.7",
    "expo-linking": "~8.0.9",
    "expo-router": "~6.0.15",
    "expo-splash-screen": "~31.0.11",
    "expo-status-bar": "~3.0.8",
    "expo-symbols": "~1.0.7",
    "expo-system-ui": "~6.0.8",
    "expo-web-browser": "~15.0.9",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-web": "^0.21.0",
    "react-native-webview": "13.15.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-community/datetimepicker": "8.4.4",
    "@react-native-community/slider": "5.0.1",
    "@storybook/addon-ondevice-actions": "^8.4.3",
    "@storybook/addon-ondevice-backgrounds": "^8.4.3",
    "@storybook/addon-ondevice-controls": "^8.4.3",
    "@storybook/react": "^8.4.3",
    "@storybook/react-native": "^8.4.3",
    "@testing-library/react-native": "^12.9.0",
    "@types/jest": "^29.5.12",
    "@types/lodash.flatten": "^4.4.9",
    "@types/react": "~19.1.10",
    "babel-loader": "^8.4.1",
    "eslint": "^8.57.0",
    "eslint-config-expo": "~10.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.2.1",
    "jest-expo": "~54.0.13",
    "lefthook": "^1.10.0",
    "lodash.flatten": "^4.4.0",
    "prettier": "^3.4.2",
    "react-native-svg": "15.12.1",
    "typescript": "~5.9.2"
  },
  "private": true,
  "packageManager": "yarn@4.12.0"
}
